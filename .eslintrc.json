{
	"env": {
		"browser": true,
		"es2021": true,
		"jest": true
	},
	"extends": [
		"plugin:react/recommended",
    "prettier",
		"airbnb"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": [
		"react",
    "prettier"
	],
	"rules": {
    "prettier/prettier": "error",
    "import/prefer-default-export": 0
  }
}
